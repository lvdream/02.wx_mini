<van-tabbar active="{{ active }}" bind:change="onChange">
  <van-tabbar-item icon="aim" id='excel'>Excel识别</van-tabbar-item>
  <van-tabbar-item icon="pending-orders" id='text'>文本识别</van-tabbar-item>
  <van-tabbar-item icon="records" id='english'>英文</van-tabbar-item>
</van-tabbar>
<view class="container">
  <view class="title">
    <view class="title-text">小贝复制</view>
  </view>
  <view class="desc">>>>>识别图片里的表格和文字</view>
</view>
<view class="row">
  <van-notice-bar scrollable="{{ false }}" text="{{ text }}" />
</view>

<demo-block title="" padding>
  <view class="row">
    <van-button size="large" type="primary" block custom-class="demo-margin-bottom" bindtap='preview'>预览</van-button>
    <van-button size="large" type="primary" block custom-class="demo-margin-bottom" bindtap='save'>保存</van-button>
        <van-button size="large" type="primary" block custom-class="demo-margin-bottom" open-type="share">分享</van-button>
  </view>
</demo-block>